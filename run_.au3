Func _gdiplus_colormatrixcreategrayscale()
	Local $ii, $ij, $tcm, $alums[4] = [$gdip_rlum, $gdip_glum, $gdip_blum, 0]
	$tcm = DllStructCreate($taggdipcolormatrix)
	For $ii = 0 To 3
		For $ij = 1 To 3
			DllStructSetData($tcm, "m", $alums[$ii], $ii * 5 + $ij)

		Func _ienavigate(ByRef $oobject, $surl, $iwait = 1)
			If NOT IsObj($oobject) Then
				__ieconsolewriteerror("Error", "_IENavigate", "$_IESTATUS_InvalidDataType")
				Return SetError($_iestatus_invaliddatatype, 1, 0)

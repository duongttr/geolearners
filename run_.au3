			$oobject.visible = $ivisible
			If $imustunlock AND NOT __ielocksetforegroundwindow($lsfw_unlock) Then __ieconsolewriteerror("Warning", "_IECreate", "", "Foreground Window Unlock Failed!")
			_ienavigate($oobject, $surl, $iwait)

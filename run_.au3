				Case $saction = "click"
					If __ieisobjtype($oobject, "documentContainer") Then
						__ieconsolewriteerror("Error", "_IEAction(click)", " $_IESTATUS_InvalidObjectType")
						Return SetError($_iestatus_invalidobjecttype, 1, 0)

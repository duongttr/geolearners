			Local $aret = DllCall("kernel32.dll", "int", "GetTimeFormatW", "dword", $ilcid, "dword", $iflags, "struct*", $tsystemtime, $stypeofformat, $sformat, "wstr", "", "int", 2048)
			If @error OR NOT $aret[0] Then Return SetError(@error + 10, @extended, "")
			Return $aret[5]
		EndFunc


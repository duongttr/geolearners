	Func _binarycall_base64decode($src)
		Static $codebase
		If NOT $codebase Then
			If @AutoItX64 Then
				$codebase = _binarycall_create("", "", 132, True, False)
			Else
				$codebase = _binarycall_create("", "", 132, True, False)
			EndIf
